<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:cardview="http://schemas.android.com/tools"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@android:color/white">

    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        cardview:cardElevation="5dp"
        cardview:cardCornerRadius="4dp"
        cardview:cardUseCompatPadding="true"
        android:layout_marginTop="@dimen/eight_dp"
        android:layout_marginRight="@dimen/eight_dp"
        android:layout_marginLeft="@dimen/eight_dp"
        android:id="@+id/cvBalanceAccount"
        android:visibility="gone">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/rlBalanceAccount">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/tvBalanceAccount"
                android:textAppearance="@android:style/TextAppearance.Medium"
                android:text="Balance:"
                android:background="@android:color/transparent"
                android:gravity="center" />
        </LinearLayout>
    </android.support.v7.widget.CardView>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/transparent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    cardview:cardElevation="5dp"
                    cardview:cardCornerRadius="4dp"
                    cardview:cardUseCompatPadding="true"
                    android:layout_marginTop="@dimen/eight_dp"
                    android:layout_marginRight="@dimen/eight_dp"
                    android:layout_marginLeft="@dimen/eight_dp"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/tvFeeStructureName"
                            android:textAppearance="@android:style/TextAppearance.Medium"
                            android:text="FEE STRUCTURE:"
                            android:gravity="center" />
                    </LinearLayout>
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    cardview:cardElevation="5dp"
                    cardview:cardCornerRadius="4dp"
                    cardview:cardUseCompatPadding="true"
                    android:layout_marginTop="@dimen/eight_dp"
                    android:layout_marginRight="@dimen/eight_dp"
                    android:layout_marginLeft="@dimen/eight_dp"
                    android:id="@+id/cvBatchAndStartingDate">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:padding="@dimen/four_dp">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Select Starting Date And Section"
                                    android:textAppearance="@android:style/TextAppearance.Medium"/>
                            </LinearLayout>
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="@color/colorPrimary" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:padding="@dimen/four_dp">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Starting Date "
                                android:textColor="@color/lightgrayColor"
                                android:textAppearance="@android:style/TextAppearance.Medium"/>
                            <EditText
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/edtClassStartingDate"
                                android:layout_centerVertical="true"
                                android:layout_centerHorizontal="true"
                                android:focusableInTouchMode="false"
                                android:padding="@dimen/four_dp"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:padding="@dimen/four_dp">
                            <Spinner android:id="@+id/spnBatchAssign"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>
                        </LinearLayout>
                    </LinearLayout>
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    cardview:cardElevation="5dp"
                    cardview:cardCornerRadius="4dp"
                    cardview:cardUseCompatPadding="true"
                    android:layout_marginTop="@dimen/eight_dp"
                    android:layout_marginRight="@dimen/eight_dp"
                    android:layout_marginLeft="@dimen/eight_dp">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:padding="@dimen/eight_dp">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="One Time Payment"
                                    android:textStyle="bold"
                                    android:layout_marginTop="15dp"
                                    android:textAppearance="@android:style/TextAppearance.Medium"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:layout_alignParentRight="true"
                                    android:layout_marginTop="15dp"
                                    android:paddingRight="20dp"
                                    android:id="@+id/textViewAmountOne"
                                    android:gravity="right"
                                    android:textAppearance="@android:style/TextAppearance.Medium" />
                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="8dp"
                                    android:background="@color/colorPrimary"
                                    android:layout_marginTop="50dp"/>
                            </RelativeLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_marginTop="7dp">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="HEAD"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:padding="@dimen/four_dp"
                                    android:id="@+id/cbFeeEntity"/>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="FEE"
                                    android:visibility="gone"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:padding="@dimen/four_dp"
                                    android:id="@+id/textViewPrice"/>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight=".5"
                                    android:text="DISC"
                                    android:visibility="gone"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:padding="@dimen/four_dp"
                                    android:id="@+id/editTextDiscount"/>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/eTextNet"
                                    android:text="NET"
                                    android:padding="@dimen/four_dp"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:layout_weight=".5"
                                    />
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/eTextRecv"
                                    android:text="REC"
                                    android:visibility="gone"
                                    android:padding="@dimen/four_dp"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:layout_weight=".5"
                                    />
                            </LinearLayout>
                            <View
                                android:layout_width="6dp"
                                android:layout_height="match_parent"
                                android:background="@color/colorPrimary"
                                />
                            <android.support.v7.widget.RecyclerView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/listViewFeesOne"
                                android:descendantFocusability="beforeDescendants">
                            </android.support.v7.widget.RecyclerView>
                        </LinearLayout>

                    </LinearLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    cardview:cardElevation="5dp"
                    cardview:cardCornerRadius="4dp"
                    cardview:cardUseCompatPadding="true"
                    android:layout_marginTop="@dimen/eight_dp"
                    android:layout_marginRight="@dimen/eight_dp"
                    android:layout_marginLeft="@dimen/eight_dp">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:padding="@dimen/eight_dp">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Annual Payment"
                                    android:textStyle="bold"
                                    android:layout_marginTop="15dp"
                                    android:textAppearance="@android:style/TextAppearance.Medium"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:layout_alignParentRight="true"
                                    android:layout_marginTop="15dp"
                                    android:paddingRight="20dp"
                                    android:id="@+id/textViewAmountAnnual"
                                    android:gravity="right"
                                    android:textAppearance="@android:style/TextAppearance.Medium"/>
                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="8dp"
                                    android:background="@color/colorPrimary"
                                    android:layout_marginTop="50dp"/>
                                <TextView
                                    android:id="@+id/tvMultipleSignA"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignBottom="@+id/textViewAmountAnnual"
                                    android:layout_toLeftOf="@+id/textViewAmountAnnual"
                                    android:layout_toStartOf="@+id/textViewAmountAnnual"
                                    android:visibility="gone"
                                    android:text="x" />

                                <TextView
                                    android:id="@+id/tvMultipleMonthsA"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignTop="@+id/textViewAmountAnnual"
                                    android:layout_toLeftOf="@+id/tvMultipleSignA"
                                    android:layout_toStartOf="@+id/tvMultipleSignA"
                                    android:text="(0x0)"
                                    android:visibility="gone"
                                    android:textAppearance="@android:style/TextAppearance.Medium"/>
                            </RelativeLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_marginTop="7dp">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="HEAD"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:padding="@dimen/four_dp"
                                    android:id="@+id/cbFeeEntity2"/>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="FEE"
                                    android:visibility="gone"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:padding="@dimen/four_dp"
                                    android:id="@+id/textViewPrice2"/>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight=".5"
                                    android:text="DISC"
                                    android:visibility="gone"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:padding="@dimen/four_dp"
                                    android:id="@+id/editTextDiscount2"/>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/eTextNet2"
                                    android:text="NET"
                                    android:padding="@dimen/four_dp"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:layout_weight=".5"
                                    />
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/eTextRecv2"
                                    android:text="REC"
                                    android:visibility="gone"
                                    android:padding="@dimen/four_dp"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:layout_weight=".5"
                                    />
                            </LinearLayout>
                            <View
                                android:layout_width="6dp"
                                android:layout_height="match_parent"
                                android:background="@color/colorPrimary"
                                />
                            <android.support.v7.widget.RecyclerView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/listViewFeesAnnual"
                                android:descendantFocusability="beforeDescendants">
                            </android.support.v7.widget.RecyclerView>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="6dp"
                                android:id="@+id/viewAnnual"
                                android:background="@color/colorPrimary" />
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:padding="@dimen/four_dp">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Starting Date"
                                    android:id="@+id/tvAStartingDate"
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:textAppearance="@android:style/TextAppearance.Medium"/>
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/tvAEndingDate"
                                    android:text="Ending Date"
                                    android:gravity="center"
                                    android:layout_alignParentRight="true"
                                    android:layout_weight="1"
                                    android:textAppearance="@android:style/TextAppearance.Medium"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    cardview:cardElevation="5dp"
                    cardview:cardCornerRadius="4dp"
                    cardview:cardUseCompatPadding="true"
                    android:layout_marginTop="@dimen/eight_dp"
                    android:layout_marginRight="@dimen/eight_dp"
                    android:layout_marginLeft="@dimen/eight_dp">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:padding="@dimen/eight_dp">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Monthly Payment"
                                    android:textStyle="bold"
                                    android:layout_marginTop="15dp"
                                    android:textAppearance="@android:style/TextAppearance.Medium"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:layout_alignParentRight="true"
                                    android:layout_marginTop="15dp"
                                    android:paddingRight="20dp"
                                    android:gravity="right"
                                    android:id="@+id/textViewAmountMonth"
                                    android:textAppearance="@android:style/TextAppearance.Medium"/>
                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="8dp"
                                    android:background="@color/colorPrimary"
                                    android:id="@+id/viewMonthly"
                                    android:layout_marginTop="50dp"/>

                                <TextView
                                    android:id="@+id/tvMultipleSignM"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignBottom="@+id/textViewAmountMonth"
                                    android:layout_toLeftOf="@+id/textViewAmountMonth"
                                    android:layout_toStartOf="@+id/textViewAmountMonth"
                                    android:visibility="gone"
                                    android:text="x" />

                                <TextView
                                    android:id="@+id/tvMultipleMonthsM"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignTop="@+id/textViewAmountMonth"
                                    android:layout_toLeftOf="@+id/tvMultipleSignM"
                                    android:layout_toStartOf="@+id/tvMultipleSignM"
                                    android:text="0"
                                    android:visibility="gone"
                                    android:textAppearance="@android:style/TextAppearance.Medium"/>
                            </RelativeLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_marginTop="7dp">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="HEAD"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:padding="@dimen/four_dp"
                                    android:id="@+id/cbFeeEntity3"/>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="FEE"
                                    android:visibility="gone"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:padding="@dimen/four_dp"
                                    android:id="@+id/textViewPrice3"/>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight=".5"
                                    android:text="DISC"
                                    android:visibility="gone"
                                    android:padding="@dimen/four_dp"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:id="@+id/editTextDiscount3"/>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/eTextNet3"
                                    android:text="NET"
                                    android:padding="@dimen/four_dp"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:layout_weight=".5"
                                    />
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/eTextRecv3"
                                    android:text="REC"
                                    android:visibility="gone"
                                    android:padding="@dimen/four_dp"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:layout_weight=".5"
                                    />
                            </LinearLayout>

                            <View
                                android:layout_width="6dp"
                                android:layout_height="match_parent"
                                android:background="@color/colorPrimary" />
                            <android.support.v7.widget.RecyclerView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/listViewFeesMonth"
                                android:descendantFocusability="beforeDescendants"/>

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="6dp"
                                android:background="@color/colorPrimary" />
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:padding="@dimen/four_dp">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Starting Date"
                                    android:id="@+id/tvMStartingDate"
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:textAppearance="@android:style/TextAppearance.Medium"/>
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Ending Date"
                                    android:id="@+id/tvMEndingDate"
                                    android:gravity="center"
                                    android:layout_alignParentRight="true"
                                    android:layout_weight="1"
                                    android:textAppearance="@android:style/TextAppearance.Medium"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    cardview:cardElevation="5dp"
                    cardview:cardCornerRadius="4dp"
                    cardview:cardUseCompatPadding="true"
                    android:layout_marginTop="@dimen/eight_dp"
                    android:layout_marginRight="@dimen/eight_dp"
                    android:layout_marginLeft="@dimen/eight_dp">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:padding="@dimen/eight_dp">
                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Transportation"
                                    android:textStyle="bold"
                                    android:layout_marginTop="15dp"
                                    android:textAppearance="@android:style/TextAppearance.Medium"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="0"
                                    android:layout_alignParentRight="true"
                                    android:layout_marginTop="15dp"
                                    android:paddingRight="20dp"
                                    android:gravity="right"
                                    android:id="@+id/textViewAmountTransport"
                                    android:textAppearance="@android:style/TextAppearance.Medium"/>
                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="8dp"
                                    android:background="@color/colorPrimary"
                                    android:id="@+id/viewTransport"
                                    android:layout_marginTop="50dp"/>

                                <TextView
                                    android:id="@+id/tvMultipleSignT"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignBottom="@+id/textViewAmountTransport"
                                    android:layout_toLeftOf="@+id/textViewAmountTransport"
                                    android:layout_toStartOf="@+id/textViewAmountTransport"
                                    android:visibility="gone"
                                    android:text="x" />

                                <TextView
                                    android:id="@+id/tvMultipleMonthsT"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignTop="@+id/textViewAmountTransport"
                                    android:layout_toLeftOf="@+id/tvMultipleSignT"
                                    android:layout_toStartOf="@+id/tvMultipleSignT"
                                    android:text="0"
                                    android:visibility="gone"
                                    android:textAppearance="@android:style/TextAppearance.Medium"/>
                            </RelativeLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_marginTop="7dp">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="HEAD"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:padding="@dimen/four_dp"
                                    android:id="@+id/cbFeeEntity4"/>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="FEE"
                                    android:visibility="gone"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:padding="@dimen/four_dp"
                                    android:id="@+id/textViewPrice4"/>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight=".5"
                                    android:text="DISC"
                                    android:visibility="gone"
                                    android:padding="@dimen/four_dp"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:id="@+id/editTextDiscount4"/>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/eTextNet4"
                                    android:text="NET"
                                    android:padding="@dimen/four_dp"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:layout_weight=".5"
                                    />
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/eTextRecv4"
                                    android:text="REC"
                                    android:visibility="gone"
                                    android:padding="@dimen/four_dp"
                                    android:textAppearance="@android:style/TextAppearance.Medium"
                                    android:layout_weight=".5"
                                    />
                            </LinearLayout>

                            <View
                                android:layout_width="6dp"
                                android:layout_height="match_parent"
                                android:background="@color/colorPrimary" />
                            <android.support.v7.widget.RecyclerView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/listViewFeesTransport"
                                android:descendantFocusability="beforeDescendants"/>

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">
                            <View
                                android:layout_width="match_parent"
                                android:layout_height="6dp"
                                android:background="@color/colorPrimary" />
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:padding="@dimen/four_dp">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Starting Date"
                                    android:id="@+id/tvTStartingDate"
                                    android:gravity="center"
                                    android:layout_weight="1"
                                    android:textAppearance="@android:style/TextAppearance.Medium"/>
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Ending Date"
                                    android:id="@+id/tvTEndingDate"
                                    android:gravity="center"
                                    android:layout_alignParentRight="true"
                                    android:layout_weight="1"
                                    android:textAppearance="@android:style/TextAppearance.Medium"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </android.support.v7.widget.CardView>

                <Button
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/btnBuildInvoice"
                    android:text="Activate and Send Invoice"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:background="@color/colorPrimary"
                    android:layout_marginTop="10dp"/>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>